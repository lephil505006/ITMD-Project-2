#main-navbar{
  background-color: #3A3E59;
  overflow:hidden;
  border-radius: 25px;
}

#navbar-container{
  float:left;
  width: 100%;
  position: relative;
}

#navbar-container ul{
  list-style-type: none;
  margin: 0;
  padding: 0;
  position: relative;
  left: 50%;
  float: left;
}

#navbar-container ul li{
  float: left;
  position: relative;
  right: 50%;
  display: block;
}

#navbar-container ul li a{
  display: block;
  padding: 20px 150px;
  text-decoration: none;
  color: #FFB567;
  font-size: 150%;
}